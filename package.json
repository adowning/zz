{
  "name": "cashflowcasino",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "devz": "concurrently --names \"FRONTEND,API,ZERO\" --prefix-colors \"blue,green,yellow\" \"bun dev:frontend\" \"bun --cwd api/ dev\" \"bun zero-cache-dev\"",
    "dev": "concurrently --names \"FRONTEND,API\" --prefix-colors \"blue,green\" \"bun dev:frontend\" \"bun --cwd api/ dev\"",
    "dev:all": "node dev-all.js",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "build:test": "vite build --mode test",
    "bump": "bun update @varlet/* @configurajs/* rattail api-farmer --latest",
    "dev:frontend": "vite",
    "initializeVipRanks": "bun run api/src/lib/vip.seed.ts",
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "preview": "vite preview",
    "generate:zero": "drizzle-zero generate --format",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cypress open",
    "test:e2e-run": "cypress run",
    "type-check": "vue-tsc --noEmit",
    "dev:api": "bun --watch api/src/index.ts",
    "tsgo": "tsgo",
    "check-types": "tsgo --noEmit",
    "start": "bun src/index.ts",
    "lint:fix": "bun lint --fix",
    "db:reset": "bun run scripts/seeds/reset.ts",
    "generate": "rm -rf api/db/zod && bunx prisma generate && mv ./shared/schema.ts ./shared/zero.ts && bunx orval --config orval.config.ts",
    "push": "bun --bun drizzle-kit push",
    "migrate": "bun --bun drizzle-kit migrate",
    "seed": "bun scripts/seed.ts",
    "studio": "bun --bun drizzle-kit studio",
    "mcp:dev": "bun run mcp/logstream/src/server.ts",
    "logs:forward": "bun run scripts/log-forwarder.ts",
    "dev:with-logs": "bun run src/index.ts 2>&1 | bun run scripts/log-forwarder.ts",
    "generate-zero-schema": "drizzle-zero generate --format -o zero/schema.gen.ts"
  },
  "lint-staged": {
    "*.${vue,ts,tsx,js,jsx,less,css}": "bun format",
    "*.{vue,ts,tsx,js,jsx}": "bun lint"
  },
  "trustedDependencies": [
    "@rocicorp/zero-sqlite3"
  ],
