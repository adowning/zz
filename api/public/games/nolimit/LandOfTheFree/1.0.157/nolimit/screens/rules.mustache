    {{#replaceSlotsInGameRules}}
        <li>{{#tr}}Land Of The Free is a video instant game with following feature(s):{{/tr}}</li>
    {{/replaceSlotsInGameRules}}
    {{^replaceSlotsInGameRules}}
        <li>{{#tr}}Land Of The Free  is a video slot with following feature(s):{{/tr}}</li>
    {{/replaceSlotsInGameRules}}
