  <div class="boostedBetAllowed">{{#tr}}- xBet: Asscar Mode{{/tr}}</div>
  <div>{{#tr}}- Wild & Stacked Symbols{{/tr}}</div>
  <div>{{#tr}}- Conveyor Belt{{/tr}}</div>
  <div>{{#tr}}- Scatter{{/tr}}</div>
  <div>{{#tr}}- Tsunami Spins{{/tr}}</div>
  <div>{{#tr}}- Submerged Spins{{/tr}}</div>
    {{^replaceWincapInfo}}<div>{{#tr}}- Pearly Gates{{/tr}}</div>{{/replaceWincapInfo}}
